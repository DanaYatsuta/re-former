<%= form_with model: @user do |form| %>
    <%= form.label :username %>
    <%= form.text_field :username, placeholder: "example username" %><br><br>
    <%= form.label :email %>
    <%= form.text_field :email, placeholder: "example@example.com" %><br><br>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <%= form.submit%>
<% end %>